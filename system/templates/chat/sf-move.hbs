{{!
{Name: "Face Danger", Source: {…}, Stats: Array(5), Text: "When …", …}
}}

<div class='flexcol'>
  <h3>{{move.name}}</h3>

  <div
    class='dice-result dice-tooltip dice-roll ironsworn-roll'
    data-move='{{move.Name}}'
  >
    <div class='move-outcome'>
      {{{stripTables (enrichMarkdown move.data.data.Text)}}}
    </div>
  </div>

  {{#if nextOracles}}
    <hr />
    {{#each nextOracles}}
      <button
        class='starforged__oracle__roll icon-button clickable isicon-d10-tilt'
        data-tableid='{{id}}'
      >
        {{name}}
      </button>
    {{/each}}
  {{/if}}

</div>